{
  "name": "picsmasher",
  "version": "1.0.0",
  "description": "![License](https://img.shields.io/badge/License-Apache%202.0-blue.svg)",
  "main": "effects.js",
  "scripts": {
    "build": "webpack",
    "test": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!(sortablejs)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "moduleNameMapper": {
      "^sortablejs$": "<rootDir>/__mocks__/sortablejs.js"
    },
    "snapshotSerializers": [
      "<rootDir>/jest.imageDataSerializer.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./test-results",
          "outputName": "jest-junit.xml"
        }
      ]
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/helpers/"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@eslint/js": "^9.13.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "canvas": "^2.11.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^3.3.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "sortablejs": "^1.15.3",
    "util": "^0.12.5"
  }
}
